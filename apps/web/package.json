{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.local tsup-node server/index.ts --watch --ignore-watch .next --ignore-watch src --onSuccess 'node dist/index.js'",
    "build:server": "env-cmd -f .env.${APP_PROFILE} tsup-node server/index.ts --minify",
    "build": "yarn build:server && yarn build:next",
    "start": "env-cmd -f .env.${APP_PROFILE} node dist/index.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "koa": "^2.13.4",
    "koa-morgan": "^1.0.1",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ui": "*"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-morgan": "^1.0.5",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "next-transpile-modules": "9.0.0",
    "tsconfig": "*",
    "tsup": "^6.0.1",
    "typescript": "^4.5.3"
  }
}
